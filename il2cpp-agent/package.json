{
  "name": "frida-agent-example",
  "version": "1.0.0",
  "description": "Example Frida agent written in TypeScript",
  "private": true,
  "main": "agent/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "frida-compile agent/index.ts -o _agent.js -c",
    "watch": "frida-compile agent/index.ts -o _agent.js -w",
    "spawn": "frida -U -f com.wildlife.games.battle.royale.free.zooba -l _agent.js"
  },
  "devDependencies": {
    "@types/frida-gum": "^18.1.0",
    "@types/node": "^18.0.6",
    "frida-compile": "^10.2.4",
    "frida-il2cpp-bridge": "^0.7.13"
  }
}
